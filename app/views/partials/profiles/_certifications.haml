.col-md-10.col-md-offset-1.text-center.top
  %h2 Certifications
  %hr
.col-md-10.col-md-offset-1.text-center#cert
  - if action.eql?("edit")
    = render partial: "partials/profiles/certifications_edit"
  - else
    - if @certifications.empty?
      .alert-info No certifications have been added.
    - else
      = render partial: "partials/profiles/certifications_display"
